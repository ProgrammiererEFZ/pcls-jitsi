{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "iam:*",
                "ec2:*",
                "elasticloadbalancing:*",
                "autoscaling:*",
                "sns:*",
                "cloudwatch:*",
                "acm:ListCertificates",
                "events:PutRule",
                "events:PutTargets",
				"events:RemoveTargets",
				"events:DeleteRule"
            ],
            "Resource": "*"
        }
    ]
}